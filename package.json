{
  "name": "@vimlet/meta",
  "version": "2.0.15",
  "description": "",
  "main": "./src/node/index.js",
  "bin": {
    "vimlet-meta": "./src/node/index.js"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "format:js": "alpha format-js",
    "format:html": "alpha format-html",
    "format:css": "alpha format-css",
    "format": "run-s format:js format:html format:css",
    "test": "alpha test",
    "doc:api": "alpha doc api -i **/*.js,**/*.html -o ../docs/api",
    "doc:article": "alpha doc article -i ../docs/md/**/*.md -o ../docs/article",
    "doc": "run-s doc:api doc:article",
    "readme": "alpha readme",
    "badges": "alpha badges -o badges"
  },
  "author": "",
  "license": "MIT",
  "optionalDependencies": {
    "@vimlet/alpha": "0.1.29"
  },
  "dependencies": {
    "@vimlet/io": "0.0.7",
    "@vimlet/cli": "1.2.4",
    "fs-extra": "4.0.1",
    "js-yaml": "3.8.4",
    "mkdirp": "0.5.1",
    "glob-watcher": "4.0.0"
  }
}